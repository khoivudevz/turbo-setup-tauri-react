{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "turbo-setup-tauri",
	"version": "0.1.0",
	"identifier": "turbo-setup.vercel.app",
	"build": {
		"frontendDist": "../build",
		"devUrl": "http://localhost:8000",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "Turbo setup Tauri x React 19, TypeScript & Tailwind and more...",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
